{
    "title": "欣和官网前端交接文件",
    "description": "记录欣官网前端开发工具及技术",
    "author": "Telliex & Fish",
    "output.name": "",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "root": ".",
    "structure": {
        "readme": "README.md"
    },
    "links": {
        "sidebar": {
            "Shinho": "http://shinho.com.cn/"
        }
    },
    "plugins": [
        "-sharing", 
        "anchors","ace",
        "jsfiddle",
        "anchor-navigation",
        "prism", 
        "-highlight",
        "emphasize",
        "tbfed-pagefooter",
        "copy-code-button",
        "splitter",
        "alerts"
        ],
    "pluginsConfig": {
          "theme-default": {
            "showLevel": true
          },
          "jsfiddle":{
            "type":"script",
            "tabs":["result","js","css", "html"],
            "height": "500",
            "width": "500",
            "fontColor": "00FF00"
          },
          "tbfed-pagefooter": {
          "copyright":"&copy Archex Star Team",
          "modify_label": "last modified：",
          "modify_format": "YYYY-MM-DD HH:mm:ss"
          },
          "prism": {
            "css": [
              "prismjs/themes/prism-tomorrow.css"
            ]
          }
    }
}